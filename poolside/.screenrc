term                rxvt-unicode-256color

startup_message     off
vbell               off
autodetach          on
altscreen           on
nonblock            on
defscrollback       10000
attrcolor           b ".I"

caption always "%{= kg} "
hardstatus  alwayslastline "%{= kw} %-w %{= rw} %t %{-} %+w %= %{= wg}"
